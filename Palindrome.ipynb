   "source": [
    "### Write a program to check palindrome."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "3f6afa9b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the value : racecar\n",
      "Yes racecar it is palindrome !\n"
     ]
    }
   ],
   "source": [
    "def palin(x):\n",
    "      reverse = x[::-1]\n",
    "      if(x == reverse):\n",
    "         print(f\"Yes {x} it is palindrome !\")\n",
    "      else:\n",
    "         print(f\"No {x} is not palindrome !\")\n",
    "\n",
    "x = input(\"Enter the value : \")\n",
    "palin(x)"
   ]
  }
